{
    "version": 2,
    "builds": [
        {
            "src": "src/APP.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "src/**/*.ts"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/APP.ts"
        }
    ]
}